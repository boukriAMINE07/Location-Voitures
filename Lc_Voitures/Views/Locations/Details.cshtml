@model Lc_Voitures.Models.Location

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<hr />
<h2 style="color: #6c757d;">Details Réservation</h2>



<hr />
<br />


<table class="table table-striped table-hover  ">
    <tr>
        <th>
            Utilisateur
        </th>
        <th>
            Voiture
        </th>
        <th>
            Matricule
        </th>
        <th>
            Date Debut
        </th>

        <th>
            Date Fin
        </th>

        <th>
            Action
        </th>



    </tr>
    <tr>
        <td>
            @Html.DisplayFor(model => model.User.nom_Complet)

        </td>
        <td>
            @Html.DisplayFor(model => model.Voiture.Modele.nom)
        </td>
        <td>
            @Html.DisplayFor(model => model.Voiture.matricule)
        </td>
        <td>
            @Html.DisplayFor(model => model.StartDate)
        </td>


        <td>

            @Html.DisplayFor(model => model.EndDate)
        </td>


        <td>
            @Html.ActionLink("Modifier", "Edit", new { id = Model.locationID }, new { @class = "btn btn-outline-info" })
            @Html.ActionLink("Annuler", "Index", new { id = Model.locationID }, new { @class = "btn btn-outline-dark" })

        </td>
    </tr>
</table>

